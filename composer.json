{
  "name": "liga/test",
  "description": "PhpTest",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "lukianovva",
      "email": "lukianovva@example.com"
    }
  ],
  "require": {
    "php": "^8.3.2",
    "ext-pdo": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Review\\": "review/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "php vendor/bin/phpunit --do-not-cache-result --colors=always --no-coverage"
    ]
  }
}